{
  "name": "cachefactory",
  "description": "CacheFactory is a very simple and useful cache for the browser.",
  "version": "1.4.1",
  "homepage": "https://github.com/jmdobry/CacheFactory",
  "main": "dist/cachefactory.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmdobry/CacheFactory.git"
  },
  "author": "CacheFactory project authors",
  "license": "MIT",
  "keywords": [
    "cache"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "localStorage",
      "window",
      "sessionStorage"
    ]
  },
  "scripts": {
    "lint": "standard src/**/*.js",
    "bundle": "webpack --config webpack.config.js",
    "min": "uglifyjs dist/cachefactory.js -o dist/cachefactory.min.js --source-map dist/cachefactory.min.map --source-map-url cachefactory.min.map -v -m -c",
    "gzip": "echo gzipped size: $(cat dist/cachefactory.min.js | gzip -f9 | wc -c)kb",
    "build": "npm run lint && npm run bundle && npm run min",
    "karma": "karma start",
    "test": "npm run build && npm run karma",
    "ci": "npm test && cat ./coverage/PhantomJS*/lcov.info | coveralls || true"
  },
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "es6-promise": "3.1.2",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon": "1.0.4",
    "mocha": "2.4.5",
    "phantomjs-prebuilt": "2.1.7",
    "sinon": "1.17.3",
    "standard": "6.0.8",
    "uglify-js": "2.6.2",
    "webpack": "1.12.14",
    "yabh": "1.1.0"
  }
}
