{
  "name": "cachefactory",
  "description": "CacheFactory is a very simple and useful cache for the browser.",
  "version": "1.4.0",
  "homepage": "https://github.com/jmdobry/CacheFactory",
  "main": "dist/cachefactory.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmdobry/CacheFactory.git"
  },
  "author": {
    "name": "Jason Dobry",
    "url": "http://www.pseudobry.com",
    "email": "jason.dobry@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "cache"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "localStorage",
      "window",
      "sessionStorage"
    ]
  },
  "scripts": {
    "lint": "standard src/**/*.js",
    "bundle": "webpack --config webpack.config.js",
    "min": "uglifyjs dist/cachefactory.js -o dist/cachefactory.min.js --source-map dist/cachefactory.min.map -v -m -c",
    "gzip": "echo gzipped size: $(cat dist/cachefactory.min.js | gzip -f9 | wc -c)kb",
    "build": "npm run lint && npm run bundle && npm run min",
    "karma": "karma start",
    "test": "npm run build && npm run karma",
    "ci": "npm test && cat ./coverage/PhantomJS*/lcov.info | coveralls || true"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.4.1",
    "codacy-coverage": "1.1.3",
    "coveralls": "2.11.6",
    "es6-promise": "3.0.2",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-firefox-launcher": "0.1.7",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-sinon": "1.0.4",
    "mocha": "2.3.4",
    "phantomjs": "1.9.19",
    "sinon": "1.17.2",
    "standard": "5.4.1",
    "uglify-js": "2.6.1",
    "webpack": "1.12.11",
    "yabh": "1.1.0"
  }
}
